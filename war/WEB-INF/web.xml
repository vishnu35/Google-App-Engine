<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>FileListServlet</servlet-name>
		<servlet-class>com.vkrish11_caakula_vaniredd.gae.FileListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileListServlet</servlet-name>
		<url-pattern>/FileList</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FileCreateServlet</servlet-name>
		<servlet-class>com.vkrish11_caakula_vaniredd.gae.FileCreateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileCreateServlet</servlet-name>
		<url-pattern>/FileCreate</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FileUploadServlet</servlet-name>
		<servlet-class>com.vkrish11_caakula_vaniredd.gae.FileUploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileUploadServlet</servlet-name>
		<url-pattern>/FileUpload</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FileDeleteServlet</servlet-name>
		<servlet-class>com.vkrish11_caakula_vaniredd.gae.FileDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileDeleteServlet</servlet-name>
		<url-pattern>/FileDelete</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FileViewServlet</servlet-name>
		<servlet-class>com.vkrish11_caakula_vaniredd.gae.FileViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileViewServlet</servlet-name>
		<url-pattern>/FileView</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FileFindServlet</servlet-name>
		<servlet-class>com.vkrish11_caakula_vaniredd.gae.FileFindServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileFindServlet</servlet-name>
		<url-pattern>/FileFind</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
